<ul class="lt-actions">
    <li class="select trigger-checkbox"  {{action 'toggleCheckboxesState'}}>
        <a href="" class="check">
            <span class="snf-checkbox-unchecked checkbox"></span>
        </a>
        <em>Un</em>Select all

    </li>
    {{#each a in actionsMetaLt}}
        {{#if a.actMany}}
            <li><a {{action 'openModal' a.act type model}} {{bind-attr class="a.title"}}>{{a.title}}</a></li>
        {{/if}}
    {{/each}}        
</ul>